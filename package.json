{
  "name": "recipe-book",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon",
    "build": "npm run build:api && npm run build:ui",
    "build:api": "tsc",
    "build:ui": "cd ui && npm i && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "ext": "ts"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "pg": "^8.8.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "winston": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregorychen3/recipe-book.git"
  },
