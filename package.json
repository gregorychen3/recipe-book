  "main": "index.js",
  "scripts": {
    "build": "npm run build:api && npm run build:ui",
    "build:api": "tsc",
    "build:ui": "cd ui && npm i && npm run build",
    "start": "export NODE_ENV=production && node dist/server.js",
    "start:dev": "export NODE_ENV=development && nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "exec": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "ext": "ts",
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.15",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.18.11",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.0",
    "axios": "^1.2.2",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "engines": {
    "node": "=16"
  }
